{
    "name":"Quick Search Advanced",
    "version":"1.0.2",
    "description":"Extension to retrieve google form answer and mark right answer in form itself.",
    "manifest_version":2,
    "author":"MQ",
    "browser_action":{
        "default_icon":"logo.png",
        "default_title":"Quick Search Advanced"
    },
    "icons":{
        "16":"logo.png",
        "48":"logo.png",
        "128":"logo.png"
    },
    "content_scripts": [
        {
        "matches": [
            "https://docs.google.com/forms*"
        ],
        "css": [],
        "js": [
            "content.js"
        ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "https://docs.google.com/*"
      ]

}